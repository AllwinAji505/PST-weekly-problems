class Solution {
    private boolean isVowels(char c){
        return "aeiouAEIOU".indexOf(c)!=-1;
    }
    public boolean halvesAreAlike(String s) {
        int num=s.length()/2;
        int count1=0,count2=0;
        for(int i=0;i<num;i++){
            if(isVowels(s.charAt(i))){
                count1++;
            }
            if(isVowels(s.charAt(i+num))){
                count2++;
            }
        }
        return count1==count2;
    }
}
